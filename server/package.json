{
  "name": "pawsiq-backend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --project ../tsconfig.json && tsc-alias -p ../tsconfig.json",
    "start": "node dist/server/index.js",
    "render-build": "npm run build && prisma migrate deploy --schema ../prisma/schema.prisma",
    "postinstall": "prisma generate --schema ../prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@google/generative-ai": "^0.21.0",
    "@supabase/supabase-js": "^2.46.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status": "^2.1.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.11.17",
    "prisma": "^6.14.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.3"
  }
